        "potato"
    ],
    "ingredients": {
        "kartoffel": {
            "amount": "500g"
        },
        "haehnchenbrustfilet": {
            "amount": "2"
        },
        "zucchini": {
            "amount": "1"
        },
        "porree": {
            "amount": "1"
        },
        "cremefraiche": {
            "amount": "1"
        },
        "senf-mittelscharf": {
            "amount": "1"
        },
        "bruehe": {
            "amount": "500ml"
        },
        "salz-pfeffer": {}
    },
    "preparation": [
        "{haehnchenbrustfilet} im Wok anbraten. {kartoffel} in Scheiben schneiden und zugeben.",
